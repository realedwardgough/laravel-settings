{
    "name": "egough/laravel-settings",
    "description": "Database-backed application and model settings for Laravel.",
    "keywords": [
        "laravel",
        "settings",
        "feature-flags",
        "configuration"
    ],
    "homepage": "https://github.com/realedwardgough/laravel-settings",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Egough\\LaravelSettings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Edward Gough",
            "email": "edwardggough@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Egough\\LaravelSettings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "Egough\\LaravelSettings\\Facades\\Settings",
                "Flag": "Egough\\LaravelSettings\\Facades\\Flag"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "laravel/pint": "dev-main"
    }
}
